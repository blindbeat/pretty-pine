{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": "off",
    "import/extensions": [
      "error",
      "ignorePackages"
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^next$"
      }
    ]
  }
}